        <div class="container clearfix">
            <div id="logo" class="grid_3">
                <a href="index.html"> <img src="/assets/pawlogo.png"> </a>
            </div>
            <div id="nav" class="grid_9 omega">
                <ul class="navigation">
                    <a href="login.html">
                        <li>Appointment</li>
                    </a>
                    
                    <li data-slide="1"></li>
                    <li data-slide="2"></li>
                    <li data-slide="3">Calender</li>
                    <li data-slide="4">Contact</li>
                    <li>Log Out</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="slide" id="slide1" data-slide="1" data-stellar-background-ratio="0.5">
        <div class="container clearfix">
            <div id="content" class="grid_7">
                <h1>Congrats!</h1>
                <h2>You signed up for <span>{{app_time}}!</span>!</h2>
                <h2>Can't wait to see you!</h2>
            </div>
        </div>
    </div>
    <div class="slide" id="slide2b" data-slide="2" data-stellar-background-ratio="0.5">
        <div class="container clearfix">
            <div id="content" class="grid_12">
                
            </div>
        </div>
    </div>
    <div class="slide" id="slide3" data-slide="3" data-stellar-background-ratio="0.5">
        <div class="container clearfix">
            <div id="content" class="grid_12">
                <h1>Calender goes here</h1>
                <h2>Yup, right under here</h2>
            </div>
            <div id="place-holder"><img src="../public/assets/calendar.png"></div>
            <div id="" class="grid_7">
                
                <br>
                <table id="place-holder-table" style="width:100%">
                    <tr>
                        <th>Dog</th>
                        <th>Service</th>
                        <th>Date</th>
                    </tr>
                    {{#each appointments}}
                        <tr>
                            <td>{{this.dog_name}}</td>
                            <td>{{this.service_type}}</td>
                            <td>{{this.service_date}}</td>
                        </tr>
                    {{/each}}
                    <tr>
                        <td>Sir Fluffington</td>
                        <td>Walking</td>
                        <td>October 14</td>
                    </tr>
                    <tr>
                        <td>Sam</td>
                        <td>Grooming</td>
                        <td>July 20</td>
                    </tr>
                    <tr>
                        <td>Sir Fluffington</td>
                        <td>Sitting</td>
                        <td> June 5</td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
    
    <div class="slide" id="slide4" data-slide="4" data-stellar-background-ratio="0.5">
        <div class="container clearfix">
            <div id="content" class="grid_12 contact">
                <h1>Contact us!</h1>
                <ul>
                    <h2>
                    <li>Kylie: Administrative Services Manager (951) 555-5478</li>
                    <li>Katie: Engineering Manager (951) 555-3875</li>
                    <li>Nathianel: Chief Information Officer (951) 555-8453</li>
                   <li>Taylor: Chief Marketing Officer (951) 555-4568</li>
